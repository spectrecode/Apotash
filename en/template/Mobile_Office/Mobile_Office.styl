		.subComunic
			border-top 3px solid verdeOscuro
			@media (max-width 767px)
				border none
			ul.menuSos
				@media (max-width 767px)
					flex-direction column
					align-items center
				a
					li
						height 65
						font-size 20px
						display flex
						justify-content center
						align-items center
						&.activesos
							background verdeOscuro
							color blanco
							font-family fuente_active
							@media (max-width 767px)
								background transparent
								color verdeOscuro
						@media (max-width 767px)
							width 200px
							padding 0
							display flex
							justify-content center
							align-items center
						&:hover
							color blanco
							@media (max-width 767px)
								color verdeOscuro
		.galerias
			@media (max-width 767px)
				margin 0
			.contenedorSalud
				@media (max-width 767px)
					padding 15px
				> div
					display flex
					margin-bottom 20%
					@media (max-width 767px)
						flex-direction column
						margin-bottom 0
				.box-somos
					width 55%
					font-size 20px
					color grisOscuro
					background #fff
					display flex
					align-items center
					padding 0 40px
					flex-direction column
					justify-content center
					left 15px
					position relative
					box-shadow: sombra
					@media (max-width 1199px)
						line-height 1
						padding 0 20px
					@media (max-width 991px)
						padding 0 15px
					@media (max-width 767px)
						width 100%
						left 0px
						box-shadow 0 0 0
					h4
						color verdeOscuro
						font-family fuente_tituloNoticias
						font-size 22px
						@media (max-width 991px)
							font-size 18px
					p
						@media (max-width 1199px)
							font-size 18px
						@media (max-width 991px)
							font-size 14px
						@media (max-width 767px)
							font-size 20px
				.lSSlideOuter
					width 45%
					position relative
					box-shadow: sombra
					ul.sliderUno
						display flex
						margin 0
						li
							cursor pointer
					@media (max-width 767px)
						width 100%
						margin auto
						padding 15px
						padding-bottom 0
						box-shadow 0 0 0
					.lSPager.lSpg
						@media (max-width 767px)
							width 95.5%
						@media (max-width 424px)
							width 100%
				