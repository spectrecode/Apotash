section#oficina
	@media (max-width 767px)
		background blanco
	.container-fluid
		.row
			.bannerPrincipal.sostenibilidad
				background-image url("../image/banner-creciendo.jpg")
				@media (max-width 991px)
					background-position 0px -4px
					background-repeat no-repeat
					height 325px
					background-size cover
				@media (max-width 424px)
					background-image url("../image/banner-movilStn.jpg")
					height 403px
				@media (max-width 375px)
					height 370px
				@media (max-width 320px)
					height 315px
	.container
		@media (max-width 767px)
			display flex
			flex-direction column-reverse
		ul.menuSos
			margin:50px auto
			display flex
			justify-content center
			padding 0
			@media (max-width 767px)
				flex-direction column
				align-items center
			a
				&:hover
					text-decoration none
				li
					cursor pointer
					color grisoscuro
					list-style none
					width 180px
					transition all ease-in-out .2s
					font-size 23px
					height 70px
					line-height 1
					display flex
					justify-content center
					align-items center
					&:hover
						background: verdeOscuro
						color blanco
						font-family fuente_active
						display flex
						align-items center
						justify-content center
						@media (max-width 767px)
							background blanco
							color grisoscuro
					&.activesos
						background verdeOscuro
						color blanco
						@media (max-width 767px)
							background transparent
							color grisoscuro
							font-family fuente_active
					&.activeBeca
						background verdeOscuro
						color blanco
						font-family fuente_active
						@media (max-width 767px)
							background transparent
