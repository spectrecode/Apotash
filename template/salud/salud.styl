section#salud
	.container-fluid
		.row
			.bannerPrincipal.sostenibilidad
				background-image url("../image/banner3.jpg")
	.container
		.salud
			margin-top 50px
			h3
				margin-bottom 40px
				&:after
					content: initial
		.galerias	
			.contenedorSalud
				display flex
				.box-somos
					width 55%
					font-size 20px
					color grisOscuro
					background #fff
					display flex
					align-items center
					padding 0 40px
					flex-direction column
					justify-content center
					left 15px
					position relative
				.lSSlideOuter
					width 45%
					position relative
					ul.sliderSalud
						display flex
						li
							cursor pointer